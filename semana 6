# Exemplo de estrutura de repetição (for) para imprimir os números de 1 a 5
print("Exemplo de estrutura de repetição (for):")
for i in range(1, 6):
    print(i)

# Exemplo de estrutura de tomada de decisão (if-else) para verificar se um número é par ou ímpar
print("\nExemplo de estrutura de tomada de decisão (if-else):")
numero = 7
if numero % 2 == 0:
    print(f"O número {numero} é par.")
else:
    print(f"O número {numero} é ímpar.")

# Descobrindo a ordem de precedência dos operadores lógicos em Python
# Ordem: not > and > or
# Operadores com maior precedência são avaliados primeiro

# Modificando a ordem das operações lógicas com parênteses
print("\nExemplo de modificação da ordem das operações lógicas com parênteses:")
a = True
b = False
c = True

resultado = (a or b) and (c or not b)

print(f"Resultado da expressão: {resultado}")
